# =========================================
# TRREB RESO Web API Tokens (Replace with real values)
# =========================================
IDX_TOKEN=your_idx_token_here
VOW_TOKEN=your_vow_token_here

# =========================================
# TRREB RESO Web API URLs
# =========================================
IDX_URL=https://query.ampre.ca/reso/odata/Property?$filter=ContractStatus eq 'Available'
VOW_URL=https://query.ampre.ca/reso/odata/Property?$filter=ContractStatus ne 'Available'
MEDIA_URL=https://query.ampre.ca/reso/odata/Media?$orderby=MediaModificationTimestamp%20desc

# =========================================
# Supabase Configuration
# =========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# =========================================
# Sync Configuration
# =========================================
# ISO 8601 format (UTC). Use "2020-01-01T00:00:00Z" to backfill from Jan 1 2020.
START_DATE=2024-01-01T00:00:00Z
BATCH_SIZE=10000
UPSERT_BATCH_SIZE=500
LOG_EVERY_N_BATCHES=10
MAX_RETRIES=3
RETRY_DELAY_MS=1000

# =========================================
# Debugging
# =========================================
DEBUG=false
