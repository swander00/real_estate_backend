# TRREB RESO Web API Configuration
# Copy this file to .env and fill in your actual values

# API Base URLs
IDX_BASE_URL=https://query.ampre.ca/odata/Property
VOW_BASE_URL=https://query.ampre.ca/odata/Property
MEDIA_BASE_URL=https://query.ampre.ca/odata/Media

# API Authentication Tokens
IDX_TOKEN=your_idx_token_here
VOW_TOKEN=your_vow_token_here

# Legacy MEDIA_URL (kept for backward compatibility)
MEDIA_URL=https://query.ampre.ca/odata/Media

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Sync Configuration
BATCH_SIZE=5000
NODE_ENV=development
MEDIA_SYNC_START_DATE=2024-01-01T00:00:00Z

# Database Configuration (if using direct database connection)
DATABASE_URL=your_database_url_here
